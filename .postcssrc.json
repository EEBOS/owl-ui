{
  "modules": true,
  "plugins": {
    "postcss-import": true,
    "postcss-modules": {
      "generateScopedName": "[local]",
      "localsConvention": "camelCase"
    },
    "@owlui/postcss-prefixer": {
      "prefix": "owlui-",
      "ignore": [
        "active",
        "bg-primary",
        "bg-secondary",
        "bg-success",
        "bg-danger",
        "bg-warning",
        "bg-info",
        "bg-light",
        "bg-dark",
        "btn-outline-success",
        "btn",
        "btn-close",
        "btn-close-white",
        "collapse",
        "collapsed",
        "collapse-horizontal",
        "collapsing",
        "container",
        "d-flex",
        "dropdown",
        "dropdown-item",
        "dropdown-menu",
        "disabled",
        "fade",
        "flex-column",
        "flex-grow-1",
        "flex-end",
        "flex-start",
        "form-control",
        "h5",
        "hiding",
        "invisible",
        "is-valid",
        "is-invalid",
        "invalid",
        "invalid-feedback",
        "mb-3",
        "me-auto",
        "navbar",
        "navbar-collapse",
        "navbar-dark",
        "navbar-light",
        "navbar-expand",
        "navbar-nav",
        "navbar-toggler",
        "nav-link",
        "justify-content-center",
        "justify-content-end",
        "pe-3",
        "rounded-pill",
        "show",
        "showing",
        "text-primary",
        "text-secondary",
        "text-success",
        "text-danger",
        "text-warning",
        "text-info",
        "text-light",
        "text-dark",
        "valid-feedback",
        "valid-tooltip",
        "visible",
        "visually-hidden",
        "was-validated"
      ]
    }
  }
}
