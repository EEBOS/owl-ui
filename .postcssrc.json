{
  "modules": true,
  "plugins": {
    "postcss-import": true,
    "postcss-modules": {
      "generateScopedName": "[local]",
      "localsConvention": "camelCase"
    },
    "@owlui/postcss-prefixer": {
      "prefix": "owlui-",
      "ignore": [
        "active",
        "bg-success",
        "bg-danger",
        "bg-warning",
        "bg-info",
        "btn-close",
        "btn-close-white",
        "collapse",
        "collapsed",
        "collapse-horizontal",
        "collapsing",
        "container",
        "dropdown",
        "fade",
        "hiding",
        "invisible",
        "is-valid",
        "is-invalid",
        "invalid",
        "invalid-feedback",
        "navbar",
        "navbar-collapse",
        "navbar-nav",
        "show",
        "showing",
        "text-primary",
        "text-secondary",
        "text-success",
        "text-danger",
        "text-warning",
        "text-info",
        "text-light",
        "text-dark",
        "valid-feedback",
        "valid-tooltip",
        "visible",
        "visually-hidden",
        "was-validated"
      ]
    }
  }
}
