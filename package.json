{
  "name": "@owlui/root",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EEBOS/owl-ui.git"
  },
  "author": "aubeOSG <caube@osg.ca>",
  "license": "MIT",
  "description": "Reusable React components.",
  "type": "module",
  "scripts": {
    "logs:print": "node ./scripts/pipeline/cmd.logs.js",
    "logs:clean": "node ./scripts/pipeline/cmd.logs-clean.js",
    "lint:fix": "yarn lint:fix:eslint && yarn lint:fix:prettier",
    "prelint:fix:eslint": "yarn run logs:clean",
    "lint:fix:eslint": "eslint --fix \"packages/**/*.{ts,tsx,js,jsx}\" --format compact --output-file ./logs/eslint.txt || yarn run logs:print -f ./logs/eslint.txt",
    "lint:fix:prettier": "prettier --write \"packages/**/*.{json,ts,tsx,js,jsx}\"",
    "lint:fix:styles": "stylelint --fix \"packages/**/*.scss\"",
    "prelint:check:eslint": "yarn run logs:clean",
    "lint:check": "yarn lint:check:eslint && yarn lint:check:prettier",
    "lint:check:eslint": "eslint \"packages/**/*.{ts,tsx,js,jsx}\" --format compact --output-file ./logs/eslint.txt || yarn run logs:print -f ./logs/eslint.txt",
    "lint:check:prettier": "prettier --check \"packages/**/*.{json,ts,tsx,js,jsx}\"",
    "lint:check:styles": "stylelint \"packages/**/*.scss\"",
    "setup": "lerna bootstrap --use-workspaces",
    "postinstall": "yarn run setup",
    "clean": "rimraf ./lib/dist && rimraf ./web/dist && rimraf ./packages/*/dist && rimraf ./.parcel-cache",
    "clean:deep": "yarn run clean && rimraf ./**/node_modules",
    "create:component": "node ./scripts/components/cmd.create.js",
    "postcreate:component": "yarn run update:lib",
    "update:lib": "node ./scripts/components/cmd.update-lib.js",
    "build:packages": "parcel build packages/* --no-cache",
    "prebuild": "yarn run update:lib",
    "build": "parcel build lib --no-cache --public-url ./",
    "postbuild": "node ./scripts/pipeline/post-build.js",
    "build:storybook": "build-storybook",
    "build:web": "yarn workspace @owlui/web run build",
    "start:storybook": "start-storybook -p 6006",
    "start:web": "yarn workspace @owlui/web run start",
    "prepublish:lerna": "yarn run build",
    "publish:lerna": "lerna publish",
    "republish:lerna": "yarn run prepublish:lerna && lerna publish from-package"
  },
  "workspaces": [
    "design",
    "lib",
    "packages/*",
    "plugins/*",
    "typings",
    "theme",
    "utils",
    "web"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@parcel/cache": "2.5.0",
    "@parcel/core": "2.5.0",
    "@parcel/packager-ts": "2.5.0",
    "@parcel/reporter-dev-server": "2.5.0",
    "@parcel/transformer-sass": "2.5.0",
    "@parcel/transformer-typescript-tsc": "2.5.0",
    "@parcel/transformer-typescript-types": "2.5.0",
    "@parcel/validator-typescript": "2.5.0",
    "@parcel/workers": "2.5.0",
    "@popperjs/core": "^2.11.5",
    "@storybook/addon-actions": "^6.5.0-beta.0",
    "@storybook/addon-essentials": "^6.5.0-beta.0",
    "@storybook/addon-links": "^6.5.0-beta.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.0-beta.0",
    "@storybook/manager-webpack5": "^6.5.0-beta.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.0-beta.0",
    "@types/react": "~18.0.8",
    "@types/react-bootstrap": "^0.32.29",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.3",
    "chalk": "^5.0.1",
    "css-loader": "6.7.1",
    "cssnano": "^5.1.12",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-storybook": "^0.5.11",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "js-beautify": "^1.14.0",
    "lerna": "^4.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "parcel": "2.5.0",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-modules": "^4.3.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.1",
    "require-from-string": "^2.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.45.1",
    "sass-loader": "12.6.0",
    "style-dictionary": "^3.0.3",
    "style-loader": "3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.72.0",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "bootstrap": "5.2.0-beta1",
    "react": "^18.1.0",
    "react-bootstrap": "2.4.0",
    "react-dom": "^18.1.0"
  },
  "bugs": {
    "url": "https://github.com/EEBOS/owl-ui/issues"
  },
  "homepage": "https://github.com/EEBOS/owl-ui#readme",
  "keywords": [
    "osg",
    "owl",
    "owlui",
    "owl-ui",
    "owl ui",
