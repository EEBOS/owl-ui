{
  "name": "@owlui/root",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EEBOS/owl-ui.git"
  },
  "author": "aubeOSG <caube@osg.ca>",
  "license": "MIT",
  "description": "Reusable React components.",
  "scripts": {
    "fix": "node ./scripts/pipeline/cmd.fixes.js",
    "presetup": "node ./scripts/pipeline/cmd.setup.js",
    "setup": "lerna bootstrap --use-workspaces",
    "update:component": "node ./scripts/components/cmd.update-lib.js",
    "create:component": "node ./scripts/components/cmd.create.js",
    "postcreate:component": "yarn run update:component",
    "prebuild:dist": "yarn run presetup && yarn run update:component",
    "build:dist": "node ./scripts/pipeline/cmd.dist.js",
    "prebuild:storybook": "yarn run presetup",
    "build:storybook": "build-storybook",
    "prebuild:web": "yarn run presetup && cd ./web",
    "build:web": "yarn parcel build",
    "prestart:storybook": "yarn run presetup",
    "start:storybook": "start-storybook -p 6006",
    "prestart:web": "yarn run fix",
    "start:web": "parcel serve web",
    "prepublish:lerna": "yarn run build:dist",
    "publish:lerna": "lerna publish",
    "republish:lerna": "yarn run prepublish && lerna publish from-package"
  },
  "workspaces": [
    "packages/*",
    "typings",
    "web"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@parcel/packager-ts": "^2.2.1",
    "@parcel/transformer-sass": "^2.2.1",
    "@parcel/transformer-typescript-types": "^2.2.1",
    "@storybook/addon-actions": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.8",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.3",
    "colors": "^1.4.0",
    "css-loader": "5.2.6",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "js-beautify": "^1.14.0",
    "lerna": "^4.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "parcel": "^2.2.1",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-modules": "^4.3.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.1",
    "sass": "^1.45.1",
    "sass-loader": "10.1.1",
    "style-dictionary": "^3.0.3",
    "style-loader": "2.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "bugs": {
    "url": "https://github.com/EEBOS/owl-ui/issues"
  },
  "homepage": "https://github.com/EEBOS/owl-ui#readme",
  "keywords": [
    "osg",
    "owl",
    "owlui",
    "owl-ui",
    "owl ui",
    "owl-components",
    "owl components",
    "owl-design-system",
    "owl design system",
    "components",
    "design-system",
    "design system"
  ],
  "publishConfig": {
    "access": "public"
  }
}
