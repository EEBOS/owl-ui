{
  "name": "@owlui/theme",
  "version": "0.0.19",
  "description": "",
  "homepage": "https://github.com/EEBOS/owl-ui/tree/main/packages/theme#readme",
  "license": "MIT",
  "type": "commonjs",
  "source": "src/_index.scss",
  "main": "dist/owl.theme.css",
  "sass": "src/_index.scss",
  "style": "dist/owl.theme.css",
  "targets": {
    "main": false
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "*.css"
  ],
  "owlrc": {
    "ignoreComponentLib": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EEBOS/owl-ui.git",
    "directory": "packages/theme"
  },
  "publishConfig": {
    "access": "public"
  },
  "isLibrary": true,
  "outputFormat": "commonjs",
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "@owlui/typings": "^0.0.19",
    "material-design-icons": "^3.0.1"
  },
  "parcelrc": {
    "--config": "./packages/theme/.parcelrc",
    "--no-cache": ""
  },
  "staticFiles": {
    "staticPath": "src/global/assets",
    "staticOutPath": "assets"
  }
}
